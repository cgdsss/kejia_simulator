<?xml version="1.0" ?>
<sdf version="1.5">
  <model name="kejia_base">
    <!--laser_link-->
    <link name="laser_link">
      <pose>0.2 0 0.35 0 0 0</pose>
      <gravity>false</gravity>
      <inertial>
	      <mass>0.1</mass>
      </inertial>
      <visual name="visual">
      	<geometry>
      	  <mesh>
      	    <uri>file://hokuyo.dae</uri>
      	  </mesh>
      	</geometry>
      </visual>
      <sensor type="ray" name="head_hokuyo_sensor">
        <pose>0 0 0 0 0 0</pose>
        <visualize>true</visualize>
        <update_rate>40</update_rate>
        <ray>
          <scan>
            <horizontal>
              <samples>1000</samples>
              <resolution>1</resolution>
              <min_angle>-2.356</min_angle>
              <max_angle>2.356</max_angle>
            </horizontal>
          </scan>
          <range>
            <min>0.10</min>
            <max>30.0</max>
            <resolution>0.01</resolution>
          </range>
         <!-- <noise-->
            <!--type>gaussian</type-->
            <!-- Noise parameters based on published spec for Hokuyo laser
                 achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
                 stddev of 0.01m will put 99.7% of samples within 0.03m of the true
                 reading. -->
            <!--mean>0.0</mean-->
            <!--stddev>0.01</stddev-->
          <!--/noise-->
        </ray>
        <plugin name="gazebo_ros_head_hokuyo_controller" filename="libgazebo_ros_laser.so">
          <topicName>/scan</topicName>
          <frameName>laser_link</frameName>
        </plugin>
      </sensor>
    </link>
    <!--back laser_link-->
    <link name="back_laser_link">
      <pose>-0.2 0 0.35 0 0 3.1415</pose>
      <gravity>false</gravity>
      <inertial>
	      <mass>0.1</mass>
      </inertial>
      <visual name="visual">
      	<geometry>
      	  <mesh>
      	    <uri>file://hokuyo.dae</uri>
      	  </mesh>
      	</geometry>
      </visual>
    <sensor type="ray" name="back_hokuyo_sensor">
      <pose>0 0 0 0 0 0</pose>
      <visualize>true</visualize>
      <update_rate>40</update_rate>
      <ray>
        <scan>
          <horizontal>
            <samples>1000</samples>
            <resolution>1</resolution>
            <min_angle>-2.356</min_angle>
            <max_angle>2.356</max_angle>
          </horizontal>
        </scan>
        <range>
          <min>0.10</min>
          <max>30.0</max>
          <resolution>0.01</resolution>
        </range>
       <!-- <noise-->
          <!--type>gaussian</type-->
          <!-- Noise parameters based on published spec for Hokuyo laser
               achieving "+-30mm" accuracy at range < 10m.  A mean of 0.0m and
               stddev of 0.01m will put 99.7% of samples within 0.03m of the true
               reading. -->
          <!--mean>0.0</mean-->
          <!--stddev>0.01</stddev-->
        <!--/noise-->
      </ray>
      <plugin name="gazebo_ros_back_hokuyo_controller" filename="libgazebo_ros_laser.so">
        <topicName>/back_scan</topicName>
        <frameName>back_laser_link</frameName>
      </plugin>
    </sensor>
    </link>
    <!--base_link-->
    <link name="base_link">
	    <sensor name='my_contact' type='contact'>
	    <plugin name="gazebo_ros_bumper" filename="libgazebo_ros_bumper.so">
            <alwaysOn>true</alwaysOn>
            <updateRate>5</updateRate>
            <bumperTopicName>bumper</bumperTopicName>
            <frameName>base_link</frameName>
          </plugin>
          <contact>
            <collision>collision</collision>
          </contact>
        </sensor>
      <pose>0 0 0.16 0 0 0</pose>
      <inertial>
        <mass>5</mass>
        <inertia>
          <ixx>0.13</ixx>
          <iyy>0.152</iyy>
          <izz>0.23</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="collision">
        <geometry>
          <box>
            <size>0.55 0.5 0.25</size>
          </box>
        </geometry>
      </collision>
      <visual name="visual">
        <pose>0 0 0.04 0 0 0</pose>
        <geometry>
          <box>
            <size>0.55 0.5 0.25</size>
          </box>
        </geometry>
      </visual>
    </link>
    <link name="back_wheel">
      <pose>-0.2 0 0.04 0 1.5707 1.5707</pose>
      <inertial>
        <mass>2</mass>
        <inertia>
          <ixx>0.13</ixx>
          <iyy>0.152</iyy>
          <izz>0.23</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <visual name="castor_visual">
        <geometry>
          <sphere>
            <radius>0.04</radius>
          </sphere>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/FlatBlack</name>
          </script>
        </material>
      </visual>
      <collision name="castor_collision">
        <geometry>
          <sphere>
            <radius>0.04</radius>
          </sphere>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>0</mu>
              <mu2>0</mu2>
              <slip1>1.0</slip1>
              <slip2>1.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
    </link>
    <!--right_wheel-->
    <link name="right_wheel">
      <pose>0.1 -.17 0.11 0 1.5707 1.5707</pose>
      <inertial>
        <mass>1</mass>
        <inertia>
          <ixx>0.0032</ixx>
          <iyy>0.0032</iyy>
          <izz>0.001</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="right_collision">
        <geometry>
          <cylinder>
            <radius>0.11</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.11</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/FlatBlack</name>
          </script>
        </material>
      </visual>
    </link>
    <!--left_wheel-->
    <link name="left_wheel">
      <pose>0.1 .17 0.11 0 1.5707 1.5707</pose>
      <inertial>
        <mass>1.5</mass>
        <inertia>
          <ixx>0.0032</ixx>
          <iyy>0.0032</iyy>
          <izz>0.001</izz>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyz>0</iyz>
        </inertia>
      </inertial>
      <collision name="left_collision">
        <geometry>
          <cylinder>
            <radius>0.11</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <surface>
          <friction>
            <ode>
              <mu>100000.0</mu>
              <mu2>100000.0</mu2>
              <slip1>0.0</slip1>
              <slip2>0.0</slip2>
            </ode>
          </friction>
        </surface>
      </collision>
      <visual name="visual">
        <geometry>
          <cylinder>
            <radius>0.11</radius>
            <length>0.05</length>
          </cylinder>
        </geometry>
        <material>
          <script>
            <uri>file://media/materials/scripts/gazebo.material</uri>
            <name>Gazebo/FlatBlack</name>
          </script>
        </material>
      </visual>
    </link>
    <!--left_wheel_joint-->
    <joint type="revolute" name="left_wheel_hinge">
      <pose>0 0 0.11 0 0 0</pose>
      <child>left_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    <!--right_wheel_joint-->
    <joint type="revolute" name="right_wheel_hinge">
      <pose>0 0 0.11 0 0 0</pose>
      <child>right_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    <joint type="fixed" name="back_wheel_hinge">
      <pose>0 0 0 0 0 0</pose>
      <child>back_wheel</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 1 0</xyz>
        <use_parent_model_frame>true</use_parent_model_frame>
      </axis>
    </joint>
    <!--laser_joint-->
    <joint name="hokuyo_joint" type="revolute">
      <child>laser_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 0.3</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>
    <!--back laser_joint-->
    <joint name="back_hokuyo_joint" type="revolute">
      <child>back_laser_link</child>
      <parent>base_link</parent>
      <axis>
        <xyz>0 0 0.3</xyz>
        <limit>
          <upper>0</upper>
          <lower>0</lower>
        </limit>
      </axis>
    </joint>
  <plugin name="differential_drive_controller" filename="libgazebo_ros_diff_drive.so">
      <alwaysOn>true</alwaysOn>
      <updateRate>40</updateRate>
      <leftJoint>left_wheel_hinge</leftJoint>
      <rightJoint>right_wheel_hinge</rightJoint>
      <wheelSeparation>0.34</wheelSeparation>
      <wheelDiameter>0.22</wheelDiameter>
      <torque>20</torque>
      <commandTopic>/cmd_vel</commandTopic>
      <odometryTopic>/odom</odometryTopic>
      <odometryFrame>/odom</odometryFrame>
      <odometryRate>20.0</odometryRate>
      <robotBaseFrame>/base_link</robotBaseFrame>
      <legacyMode>false</legacyMode>
  </plugin>
  </model>
</sdf>
